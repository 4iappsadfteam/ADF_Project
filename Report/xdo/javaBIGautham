package test;


import java.io.IOException;
import java.io.StringReader;
import java.util.logging.Level;
import java.util.logging.Logger;
import javax.xml.parsers.DocumentBuilder;
import javax.xml.parsers.DocumentBuilderFactory;
import javax.xml.parsers.ParserConfigurationException;
import org.w3c.dom.Document;
import org.xml.sax.InputSource;
import org.xml.sax.SAXException;


/**
 *
 * @author gautham
 */
public class Test {


    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        try {
            String xmlString = "<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n"
                    + "<!--Generated by Oracle BI Publisher -Dataengine, datamodel:_Custom_Property_Lease_Data_Model_Location_xdm -->\n"
                    + "<DATA_DS><P_LAST_UPDATE_DATE>1-1-2018</P_LAST_UPDATE_DATE>\n"
                    + "<G_1>\n"
                    + "<LOCATION_ID>300000002407892</LOCATION_ID>\n"
                    + "<LOCATION_CODE>One Palm</LOCATION_CODE>\n"
                    + "<LOCATION_NAME>One Palm</LOCATION_NAME>\n"
                    + "<ACTIVE_STATUS>A</ACTIVE_STATUS>\n"
                    + "<COUNTRY>AE</COUNTRY>\n"
                    + "<LOCATION_DETAILS_ID>300000002407891</LOCATION_DETAILS_ID>\n"
                    + "<EFFECTIVE_START_DATE>2019-08-11T00:00:00.000+00:00</EFFECTIVE_START_DATE>\n"
                    + "<EFFECTIVE_END_DATE>4712-12-31T00:00:00.000+00:00</EFFECTIVE_END_DATE>\n"
                    + "<ADDRESS_LINE_1>Office # 2602-2603, Post Box 26498 One by Omniyat</ADDRESS_LINE_1>\n"
                    + "<ADDRESS_LINE_2/>\n"
                    + "<ADDRESS_LINE_3/>\n"
                    + "<ADDRESS_LINE_4/>\n"
                    + "<TOWN_OR_CITY>Dubai</TOWN_OR_CITY>\n"
                    + "<SET_ID>300000002409665</SET_ID>\n"
                    + "</G_1>\n"
                    + "</DATA_DS>\n";
            if (xmlString.contains("<G_1>")) {
                DocumentBuilder builder
                        = DocumentBuilderFactory.newInstance().newDocumentBuilder();
                InputSource src = new InputSource();
                src.setCharacterStream(new StringReader(xmlString));
                Document doc = builder.parse(src);
                String locationId = doc.getElementsByTagName("LOCATION_ID").item(0).getTextContent();
                String locationName = doc.getElementsByTagName("LOCATION_NAME").item(0).getTextContent();
                String locationCode = doc.getElementsByTagName("LOCATION_ID").item(0).getTextContent();
                String activeStatus = doc.getElementsByTagName("ACTIVE_STATUS").item(0).getTextContent();
                String country = doc.getElementsByTagName("COUNTRY").item(0).getTextContent();
                String locationDetailsId = doc.getElementsByTagName("LOCATION_DETAILS_ID").item(0).getTextContent();
                String effectiveStartDate = doc.getElementsByTagName("EFFECTIVE_START_DATE").item(0).getTextContent();
                String effectiveEndDate = doc.getElementsByTagName("EFFECTIVE_END_DATE").item(0).getTextContent();
                String addressLine1 = doc.getElementsByTagName("ADDRESS_LINE_1").item(0).getTextContent();
                String addressLine2 = doc.getElementsByTagName("ADDRESS_LINE_2").item(0).getTextContent();
                String addressLine3 = doc.getElementsByTagName("ADDRESS_LINE_3").item(0).getTextContent();
                String addressLine4 = doc.getElementsByTagName("ADDRESS_LINE_4").item(0).getTextContent();
                String city = doc.getElementsByTagName("TOWN_OR_CITY").item(0).getTextContent();
                String setId = doc.getElementsByTagName("SET_ID").item(0).getTextContent();
                System.out.println(locationId);
            } else {
                // error in report
            }


        } catch (SAXException ex) {
            Logger.getLogger(Test.class.getName()).log(Level.SEVERE, null, ex);
        } catch (IOException ex) {
            Logger.getLogger(Test.class.getName()).log(Level.SEVERE, null, ex);
        } catch (ParserConfigurationException ex) {
            Logger.getLogger(Test.class.getName()).log(Level.SEVERE, null, ex);
        }
    }


}
 







